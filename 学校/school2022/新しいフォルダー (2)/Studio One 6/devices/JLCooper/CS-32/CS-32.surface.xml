        <!-- ******************************************************************************************** -->

		<DeviceMapping device="MixerConsole">
			<PlacementBank target="RemoteBank" pagesize="32">
				<!-- The Channel Strips -->
				<foreach variable="$channel" count="32">
					<Strip> 
						<Variant control="panButton[$channel]">
							<!-- PAN: OFF -->
							<Group>
								<Value control="fader[$channel]" param="volume"/>
							</Group>
							<!-- PAN: ON -->
							<Group>
								<Value control="fader[$channel]" param="pan"/>
							</Group>
						</Variant>
						<Value control="soloButton[$channel]"  param="solo"/>
						<Value control="muteButton[$channel]"  param="mute"/>
						<Value control="armButton[$channel]"   param="recordArmed"/>
						<Value control="trackButton[$channel]" param="selected"/>
					</Strip>
				</foreach>
			</PlacementBank>
		</DeviceMapping>

